# Configuration file for Pinterest Scraper and Image Analyzer
# Copy this file to .env and adjust as needed

# Pinterest Scraper Settings
PINTEREST_OUTPUT_DIR=pinterest_images
PINTEREST_RATE_LIMIT=2.0
PINTEREST_HEADLESS=true
PINTEREST_SCROLL_COUNT=3

# Image Analyzer Settings
ANALYZER_USE_GPU=false
ANALYZER_OUTPUT_FORMAT=json

# Optional: Tesseract path (uncomment if needed)
# TESSERACT_CMD=/usr/bin/tesseract
# TESSERACT_CMD=C:\Program Files\Tesseract-OCR\tesseract.exe  # Windows
